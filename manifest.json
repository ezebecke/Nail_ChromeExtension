{
  "name": "Nail Keeper",
  "version": "1.1",
  "description":
    "Helping you keeping your hands on the keyboard and not your mouth!",
  "icons": {"16": "img/nailimg.png", "48": "img/nailimg.png", "128": "img/nailimg.png"},
  "permissions": [
    "notifications",
    "storage"
  ],
  "browser_action": {
    "default_title": "Pop up Title",
    "default_popup": "popup.html"
  },
  "background": { "scripts": ["background.js"] },
  "manifest_version": 2,


  "web_accessible_resources": [
    "img/nailimg.png"
  ]
}
